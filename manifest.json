{
    "name": "Grover Server Switcher",
    "version": "0.1.3",
    "description": "It switches between Grover servers.",
    "permissions": ["declarativeContent", "storage", "webRequest", "webRequestBlocking", "*://*.getgrover.com/", "*://*.grover.com/"],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "page_action": {
      "default_popup": "popup.html"
    },
    "manifest_version" : 2
  }